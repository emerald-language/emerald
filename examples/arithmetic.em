def test_arithmetic
  a : int = 10
  b : int = 20
  
  add_result : int = a + b
  sub_result : int = a - b
  mul_result : int = a * b
  div_result : int = b / a
  mod_result : int = b % a
  
  eq_result : bool = a == b
  ne_result : bool = a != b
  lt_result : bool = a < b
  gt_result : bool = a > b
end
